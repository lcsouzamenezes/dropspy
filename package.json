{
  "name": "dropspy",
  "version": "1.0.0",
  "description": "cloud based shopify store spy system",
  "repository": {
    "type": "git",
    "url": "https://github.com/instigaremidia/dropspy-typescript.git"
  },
  "author": {
    "name": "Lucas Vieira",
    "email": "lucasvtiradentes@gmail.com"
  },
  "keywords": [
    "dropspy",
    "typescript",
    "pupeteer",
    "alihunter",
    "shopify",
    "ecommerce",
    "cloud computing"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "main": "src/index.ts",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "dev": "ts-node-dev --transpile-only --respawn --ignore-watch node_modules src/index.ts",
    "test": "jest",
    "commit": "git-cz",
    "docker": "docker build . -t dropspy",
    "start_dev": "ts-node src/index.ts",
    "start_prod": "node dist/src/index.js",
    "start": "npm run start_dev",
    "prebuild": "npm run clean_dist",
    "build": "node ./node_modules/typescript/bin/tsc",
    "postbuild": "npm run copy_files_to_dist && npm run copy_extensions_to_dist",
    "clean_dist": "node scripts/remove-old-dist-folder.js",
    "copy_files_to_dist": "node scripts/copy-files-to-dist.js",
    "copy_extensions_to_dist": "node scripts/copy-entensions-to-dist-folder.js"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cli-color": "^2.0.3",
    "cluster": "^0.7.7",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "googleapis": "^105.0.0",
    "mongodb": "^4.7.0",
    "mongoose": "^6.4.3",
    "node-os-utils": "^1.3.7",
    "puppeteer-extra": "^3.3.4",
    "puppeteer-extra-plugin-stealth": "^2.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/axios": "^0.14.0",
    "@types/cli-color": "^2.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/mongodb": "^4.0.7",
    "@types/mongoose": "^5.11.97",
    "@types/pino": "^7.0.5",
    "@types/pino-pretty": "^4.7.5",
    "@types/puppeteer": "^5.4.6",
    "commitizen": "^4.2.4",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "pino-pretty": "^8.1.0",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.7.4"
  }
}
