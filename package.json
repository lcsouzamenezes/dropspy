{
  "name": "dropspy",
  "version": "1.0.5",
  "description": "cloud based shopify stores spy system",
  "repository": {
    "type": "git",
    "url": "https://github.com/instigaremidia/dropspy-typescript.git"
  },
  "author": {
    "name": "Lucas Vieira",
    "email": "lucasvtiradentes@gmail.com"
  },
  "keywords": [
    "dropspy",
    "typescript",
    "pupeteer",
    "alihunter",
    "shopify",
    "ecommerce",
    "cloud computing"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "configfile": "./configs/app-configs.json"
  },
  "main": "src/index.ts",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --respawn --ignore-watch node_modules src/index.ts",
    "test": "jest",
    "commit": "git-cz",

    "prebuild": "node ./scripts/build/pre-build.js",
    "build": "node ./node_modules/typescript/bin/tsc",
    "postbuild": "node ./scripts/build/post-build.js",

    "docker_build": "docker build -t dropspy .",
    "docker_pre_run": "bash ./dist/scripts/run/clean-xbfb-files.sh",

    "start_prod": "node dist/src/index.js",
    "start_dev": "ts-node src/index.ts",
    "start": "npm run start_dev"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cli-color": "^2.0.3",
    "cluster": "^0.7.7",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "mongoose": "^6.4.3",
    "node-os-utils": "^1.3.7",
    "puppeteer-extra": "^3.3.4",
    "puppeteer-extra-plugin-stealth": "^2.11.0",
    "unzipper": "^0.10.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/axios": "^0.14.0",
    "@types/cli-color": "^2.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/mongoose": "^5.11.97",
    "@types/puppeteer": "^5.4.6",
    "commitizen": "^4.2.4",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
